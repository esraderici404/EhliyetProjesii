@using EntityLayer
@model List<Randevu>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Randevu Listesi</title>
</head>
<body>


    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Randevu Tablosu </h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                            <th>Mail</th>
                            <th>CoursesType</th>
                            <th>CarType</th>
                            <th>Message</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var iteem in Model)
                        {
                            <tr>
                                <td>@iteem.ID</td>
                                <td>@iteem.Name</td>
                                <td>@iteem.Mail</td>
                                <td>@iteem.CoursesType</td>
                                <td>@iteem.CarType</td>
                                <td>@iteem.Message</td>
                            </tr>

                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
</html>

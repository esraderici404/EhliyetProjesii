@using EntityLayer
@model List<Hakkımızda>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Hakkımızda Listesi</title>
</head>
<body>


    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Hakkımızda Tablosu </h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Title</th>
                            <th>Description</th>
                            <th>Services1</th>
                            <th>Services2</th>
                            <th>Services3</th>
                            <th>Services4</th>
                            <th>phonenumber</th>
                            <th>Picture1</th>
                            <th>Picture2</th>
                            <th>Buton</th>
                            
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var iteem in Model)
                        {
                                <tr>
                                    <td>@iteem.ID</td>
                                    <td>@iteem.Title</td>
                                    <td>@iteem.Description</td>
                                    <td>@iteem.Services1</td>
                                    <td>@iteem.Services2</td>
                                    <td>@iteem.Services3</td>
                                    <td>@iteem.Services4</td>
                                    <td>@iteem.phonenumber</td>
                                    <td>@iteem.Picture1</td>
                                    <td>@iteem.Picture2</td>
                                    <td>@iteem.Buton</td>
                                    





                                </tr>

                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
</html>

